<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners
                android:bottomRightRadius="@dimen/tempSwitcherCornersRadius"
                android:topRightRadius="@dimen/tempSwitcherCornersRadius"
                />
            <solid android:color="@color/tempSwitcherCheckedColor" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners
                android:bottomRightRadius="@dimen/tempSwitcherCornersRadius"
                android:topRightRadius="@dimen/tempSwitcherCornersRadius"
                />
            <solid android:color="@color/tempSwitcherUncheckedColor" />
        </shape>
    </item>
</selector>